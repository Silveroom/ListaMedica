
<% if current_model.errors.present? %>
	<div class="alert  alert-danger">
		<button type="button" class="close" data-dismiss="alert">x</button>
		<b>Tienes errores!!</b>
		<ul>
			<% current_model.errors.messages.each do |m| %>
			<li>
			<p> <%= m %> </p>
			</li>
			<% end %>
		</ul>
	</div>
<% end %>
